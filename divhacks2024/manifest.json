{
  "manifest_version": 3,
  "name": "Website Popup",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["tabs", "activeTab"],
  "background":{
    "service_worker": "background.js"
  }
  ,"action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  }
  ,"host_permissions": [
    "facebook.com",
    "twitter.com",
    "tiktok.com",
    "instagram.com",
    "reddit.com",
    "snapchat.com",
    "youtube.com",
    "linkedin.com",
    "discord.com",
    "pinterest.com",
    "http://localhost:3000/"
  ]

}


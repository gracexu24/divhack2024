{
  "manifest_version": 3,
  "name": "Social Media Tracker",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["tabs", "activeTab"],
  "background":{
    "service_worker": "background.js"
  }
  ,"action": {
    "default_popup": "popup.html",
    "default_icon": "icon_image48.jpg"
  }
  ,"dependencies": {
    "cors": "^2.8.5", 
    "express": "^4.17.1",
    "other-package": "..."
  }
  ,"host_permissions": [
    "x.com",
    "reddit.com",
    "facebook.com",
    "twitter.com",
    "tiktok.com",
    "instagram.com",
    "reddit.com",
    "snapchat.com",
    "youtube.com",
    "linkedin.com",
    "discord.com",
    "pinterest.com"

  ]

  

}


{
  "manifest_version": 3,
  "name": "Website Popup",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["tabs", "activeTab"],
  "background":{
    "service_worker": "background.js"
  }
  ,"action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  }
  ,"host_permissions": [
    "https://facebook.com/*",
    "https://twitter.com/*",
    "https://tiktok.com/*",
    "https://instagram.com/*",
    "https://reddit.com/*",
    "https://snapchat.com/*",
    "https://youtube.com/*",
    "https://linkedin.com/*",
    "https://discord.com/*",
    "https://pinterest.com/*",
    "http://localhost:3000/"
  ]

}

